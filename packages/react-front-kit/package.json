    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.49.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@mantine/core": "6",
    "@mantine/dates": "6",
